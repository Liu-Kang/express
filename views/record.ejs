<!DOCTYPE html>
<html>
	<%- include head %>
	<link rel="stylesheet" href="/css/record.css" />
	<body>
		<section class="container">
			<% if(page.dynamicBg || page.dynamicBg == 0){ %>
				<section class="page">
				<% var bg = pageSetting.dynamicBg[page.dynamicBg] %>
				<%- include('module/' + bg , {dynamicbg:pageSetting.dynamicBg}); %>
				</section>
			<% }else if(page.staticBg || page.staticBg == 0){ %>
				<section class="page" style="background:url(<%= pageSetting.staticBg[page.staticBg] %>) no-repeat;background-size:100% 100%;">
				</section>
			<% } %>
		</section>
	</body>
</html>
